
local admins = {
    "Nanderez",
    "BOBLOXv0",
    "0_lemontrain",
    "freegirl678",
    "UltrasGroupHolder"
 }

local function isAdmin(player)

   for _, v in pairs(admins) do
       if v == player.Name then
           return true
       end
   end

   return false

end
for i,v in pairs(game.Players:GetChildren()) do
    print(v.Name)
   if isAdmin(v) then
           v.Character.Humanoid.DisplayName = "[‚≠ê] "..v.DisplayName
    end 
end
